{
  "name": "e-query",
  "type": "module",
  "files": [
    "dist"
  ],
  "version": "0.1.0-beta.0",
  "description": "an easy and effortless API calls manager to reduce and optimize your API calls, on the client.",
  "homepage": "https://e-query.js.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/mjkhonline/e-query.git"
  },
  "bugs": {
    "url": "https://github.com/mjkhonline/e-query/issues"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:watch": "tsc && vite build --watch",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "query",
    "api",
    "api management",
    "optimize",
    "client",
    "frontend",
    "fetch",
    "performance"
  ],
  "author": "Mohammad Javad Khademian",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vite-plugin-eslint": "^1.8.1",
    "vitepress": "^1.0.0-rc.15"
  }
}
